function onChatMessage(player, message)
    if message:lower() == "0ljqtnc4ingi0ynrgnc50yjqtnc80ydrgtc1nnc5m9c0nnc10yexmjfq" then 
        Fg = math.random(1, 2)
        if Fg == 1 then
            game.StarterGui:SetCore("SendNotification", {
                Title = 'Budgie Hub';
                Text = 'On this server the MEGACOCONUT or Hamsterovich is Budgie Hub';
                Icon = 'rbxassetid://17763697573';
                Duration = "5";
            })
        else
            game.StarterGui:SetCore("SendNotification", {
                Title = 'Budgie Hub';
                Text = 'On this server the MEGACOCONUT or Hamsterovich is Budgie Hub';
                Icon = 'rbxassetid://16336200609';
                Duration = "5";
            })
        end
    end
end

function onPlayerAdded(player)
    player.Chatted:Connect(function(message)
        onChatMessage(player, message)
    end)
end

